<% @presses.each do |press| %>
    <h5><%= press.location %></td></h5>
    <p class="info"><%= press.content %></p>
    <strong><%= press.reviewer %></strong>
    <p><a href="<%= press.readmoreurl %>"><%= press.readmoreurl %></a></p>
    <p>
    <%= link_to 'Show', press %><%= link_to 'Edit', edit_press_path(press) %><%= link_to 'Destroy', press, confirm: 'Are you sure?', method: :delete %>
    </p>
<% end %>

<br />

<%= link_to 'New Press', new_press_path %>
