<h1>Listing app_images</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Main screen</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_images.each do |app_image| %>
  <tr>
    <td><%= app_image.url %></td>
    <td><%= app_image.main_screen %></td>
    <td><%= link_to 'Show', app_image %></td>
    <td><%= link_to 'Edit', edit_app_image_path(app_image) %></td>
    <td><%= link_to 'Destroy', app_image, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App image', new_app_image_path %>
